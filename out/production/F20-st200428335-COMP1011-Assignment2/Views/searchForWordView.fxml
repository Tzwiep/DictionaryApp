      <JFXTextField fx:id="wordInputTextField" layoutX="220.0" layoutY="106.0" unFocusColor="#c9c9c9" />
      <JFXListView fx:id="wordListView" layoutX="51.0" layoutY="174.0" prefHeight="195.0" prefWidth="491.0" />
      <JFXButton buttonType="RAISED" layoutX="404.0" layoutY="99.0" text="Search">
         <ripplerFill>
            <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
